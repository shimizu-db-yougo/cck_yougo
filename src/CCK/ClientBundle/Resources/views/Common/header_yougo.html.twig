								<select class="form-control js-chu input-sm" name="chu">
									<option value="0">--------------</option>
									{% for chu in chu_list %}
										<option value="{{ chu.id }}" {{ chu.id == select_chu ? " selected " : '' }}">{{ replaceTag(chu.name)|raw }}</option>
									{% endfor %}
								</select>
							</div>
							<label class="col-sm-1">{{ 'text.yougo.head.ko'|trans }}</label>
							<div class="col-sm-3">
								<select class="form-control js-ko input-sm" name="ko">
									<option value="0">--------------</option>
									{% for ko in ko_list %}
										<option value="{{ ko.id }}" {{ ko.id == select_ko ? " selected " : '' }}">{{ replaceTag(ko.name)|raw }}</option>
