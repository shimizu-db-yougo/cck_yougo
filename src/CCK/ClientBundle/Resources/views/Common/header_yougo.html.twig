							</div>
							<label class="col-sm-1">{{ 'text.yougo.curriculum'|trans }}</label>
							<div class="col-sm-2">
								<select class="form-control js-cur input-sm" name="curriculum">
									<option value="0">--------------</option>
									{% for cur in cur_list %}
										<option value="{{ cur.id }}" {{ cur.id == curriculum ? " selected " : '' }}">{{ cur.name }}</option>
