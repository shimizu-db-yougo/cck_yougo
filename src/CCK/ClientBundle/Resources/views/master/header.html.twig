{% extends "CCKClientBundle::layout.html.twig" %}

{% block stylesheets %}
	{{ parent() }}
{% endblock %}

{% block contents %}
<div class="row">
<blockquote class="blockquote-bottom pdr-0">
	<p>{{ 'text.master.header'|trans }}</p>
</blockquote>
</div>

<div class="row">
	<div class="col-lg-2">
		{#{ return_msg }#}
	</div>
	<!-- <div class="col-lg-10">
		<div class="pull-right">
			<button class="btn btn-primary btn-lg js-header-new">{{ 'btn.new.default'|trans }}</button>
		</div>
	</div> -->
</div>

<div class="row">
	<div class="col-lg-12 text-center">
		{{ knp_pagination_render(pagination) }}
	</div>
</div>

<div class="row">
	<div class="col-lg-12">
		<table class="table table-responsive table-striped">
			<thead>
				<tr>
					<th width="90px" class="text-left">{{ 'text.yougo.curriculum'|trans }}</th>
					<th width="70px" class="text-left">{{ 'text.yougo.version'|trans }}</th>
					<th width="60px" class="text-center">{{ 'btn.edit'|trans }}</th>
				</tr>
			</thead>
			<tbody>
				{% for header in pagination %}
				<tr class="text-center">
					<td class="text-left">{{ header.cur_name }}</td>
					<td class="text-left">{{ header.name }}</td>
					<td>
						<div class="form-inline">
							<a href="{{ path('client.master.header.list', {'version': header.id}) }}"><button class="btn btn-primary btn-sm button_yougo_list" name="id" value="{{ header.id }}" >{{ 'btn.edit'|trans }}</button></a>
						</div>
					</td>
				</tr>
				{% endfor %}
			</tbody>
		</table>
	</div>
</div>
<div class="modal-footer">
	<div class="text-center">
		<a href="{{ path('client.master.index') }}"><div class="btn btn-default" name="back">{{ 'btn.back'|trans }}</div></a>
	</div>
</div>
{% endblock %}
{% block javascripts %}
	{{ parent() }}
	<script src="{{ asset('bundles/gimiccommon/jquery-ui/jquery-ui.min.js') }}"></script>
	<script>
		$(function(){
		});
	</script>
{% endblock %}

